{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/TerrainHeightmap/src/**",
            "${workspaceFolder}/build/_deps/**"
        ]
    },
    "configurations": [
        {
            "name": "Debug",
            "includePath": [
                "${myIncludePath}"
            ],
            "browse": {
                "path": [
                    "${myIncludePath}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "intelliSenseMode": "windows-msvc-x64",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "forcedInclude": [
                "${workspaceFolder}/TerrainHeightmap/src/Detail/TerrainHeightmapPCH.h"
            ]
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}